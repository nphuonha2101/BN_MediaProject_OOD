<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="504.0"
            prefWidth="699.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            fx:controller="project.mediaplayer.UI.MediaPlayerController">
    <children>
        <BorderPane prefHeight="504.0" prefWidth="699.0" style="-fx-background-color: #1e2228;"
                    stylesheets="@../../../css/style.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <left>
                <VBox alignment="CENTER" minHeight="250.0" prefHeight="504.0" prefWidth="176.0"
                      style="-fx-background-color: #1e2228;" stylesheets="@../../../css/style.css"
                      BorderPane.alignment="CENTER">
                    <children>
                        <Pane prefHeight="104.0" prefWidth="166.0">
                            <children>
                                <ImageView fitHeight="100.0" fitWidth="98.0" layoutX="34.0" layoutY="3.0"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../../img/logo.png"/>
                                    </image>
                                </ImageView>
                            </children>
                        </Pane>
                  <Button fx:id="homeButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#mainPlaylistLView" prefHeight="40.0" prefWidth="166.0" stylesheets="@../../../css/style.css" text="Home">
                     <graphic>
                        <ImageView fitHeight="21.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../img/icons8-home-96.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets left="40.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="favoriteButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#favoritePlaylistLView" prefHeight="40.0" prefWidth="166.0" stylesheets="@../../../css/style.css" text="Favorite">
                     <graphic>
                        <ImageView fitHeight="21.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../img/icons8-favorite-96.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets left="40.0" />
                     </padding>
                     <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </VBox.margin>
                  </Button>
                  <Button id="buttonOpenFolder" fx:id="openFolder" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#chooseFile" prefHeight="40.0" prefWidth="166.0" stylesheets="@../../../css/style.css" text="Open Folder">
                     <graphic>
                        <ImageView fitHeight="21.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../img/icons8-music-folder-96.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets left="40.0" />
                     </padding>
                     <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </VBox.margin>
                  </Button>
                  <Pane prefHeight="72.0" prefWidth="166.0" VBox.vgrow="ALWAYS" />
                  <Button id="aboutButton" fx:id="aboutButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#openAbout" prefHeight="40.0" prefWidth="166.0" stylesheets="@../../../css/style.css" text="About">
                     <graphic>
                        <ImageView fitHeight="21.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../img/icons8-info-96.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets left="40.0" />
                     </padding>
                     <VBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <opaqueInsets>
                  <Insets left="5.0" />
               </opaqueInsets>
            </VBox>
         </left>
         <center>
             <VBox id="contentVBox" prefHeight="200.0" prefWidth="100.0"
                   style="-fx-background-color: #22272e; -fx-background-radius: 10;"
                   stylesheets="@../../../css/style.css" BorderPane.alignment="CENTER">
                 <children>
                     <Label id="headerLabel" fx:id="headerLabel" prefHeight="50.0" prefWidth="443.0"
                            stylesheets="@../../../css/style.css" text="Home" textFill="#fefefe">
                         <font>
                             <Font name="System Bold" size="24.0"/>
                         </font>
                         <opaqueInsets>
                             <Insets/>
                         </opaqueInsets>
                         <VBox.margin>
                             <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                         </VBox.margin>
                     </Label>
                     <ListView id="listView" fx:id="listView" onMouseClicked="#selectedListItem" prefHeight="325.0" prefWidth="483.0" stylesheets="@../../../css/style.css" VBox.vgrow="ALWAYS">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <VBox.margin>
                         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                     </VBox.margin>
                  </ListView>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
            </VBox>
         </center>
         <bottom>
             <GridPane alignment="CENTER" prefHeight="104.0" prefWidth="699.0" style="-fx-background-color: #1e2228;"
                       stylesheets="@../../../css/style.css" BorderPane.alignment="CENTER">
                 <columnConstraints>
                     <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                     <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                     <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                 </columnConstraints>
                 <rowConstraints>
                     <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                 </rowConstraints>
                 <children>
                     <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
                     <children>
                         <ToggleButton fx:id="previousButton" accessibleRole="TOGGLE_BUTTON" mnemonicParsing="false"
                                       onAction="#previousMedia" stylesheets="@../../../css/style.css">
                             <graphic>
                                 <ImageView id="previousButton" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true"
                                            preserveRatio="true">
                                     <image>
                                         <Image url="@../../../img/icons8-skip-to-start-96.png"/>
                                     </image>
                                 </ImageView>
                             </graphic>
                             <HBox.margin>
                                 <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                             </HBox.margin>
                        </ToggleButton>
                         <ToggleButton fx:id="playButton" accessibleRole="TOGGLE_BUTTON" mnemonicParsing="false"
                                       onAction="#playMedia" stylesheets="@../../../css/style.css" HBox.hgrow="ALWAYS">
                             <graphic>
                                 <ImageView fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                                     <image>
                                         <Image url="@../../../img/icons8-play-96.png"/>
                                     </image>
                                 </ImageView>
                             </graphic>
                             <HBox.margin>
                                 <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                             </HBox.margin>
                        </ToggleButton>
                         <ToggleButton fx:id="nextButton" accessibleRole="TOGGLE_BUTTON" mnemonicParsing="false"
                                       onAction="#nextMedia" stylesheets="@../../../css/style.css">
                             <graphic>
                                 <ImageView id="nextButton" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true"
                                            preserveRatio="true">
                                     <image>
                                         <Image url="@../../../img/icons8-end-96.png"/>
                                     </image>
                                 </ImageView>
                             </graphic>
                             <HBox.margin>
                                 <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                             </HBox.margin>
                        </ToggleButton>
                     </children>
                  </HBox>
                     <HBox alignment="CENTER" prefHeight="104.0" prefWidth="224.0" GridPane.columnIndex="2">
                         <children>
                             <ToggleButton fx:id="favoriteSongButton" accessibleRole="TOGGLE_BUTTON"
                                           mnemonicParsing="false" stylesheets="@../../../css/style.css">
                                 <graphic>
                                     <ImageView id="favoriteSongButton" fitHeight="23.0" fitWidth="23.0"
                                                pickOnBounds="true" preserveRatio="true">
                                         <image>
                                             <Image url="@../../../img/icons8-favorite-96.png"/>
                                         </image>
                                     </ImageView>
                                 </graphic>
                             </ToggleButton>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                         <Label fx:id="songNameLabel" alignment="CENTER" prefHeight="35.0" prefWidth="238.0"
                                text="Song name" textFill="#fefefe">
                             <font>
                                 <Font size="14.0"/>
                             </font>
                         </Label>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
            </GridPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
